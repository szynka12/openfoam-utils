FoamFile
{
    version     2.0;
    format      binary;
    class       surfaceScalarField;
    arch        "LSB;label=32;scalar=64";
    location    "constant";
    object      meshPhi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];


internalField   uniform 0;

boundaryField
{
    yp
    {
        type            cyclic;
        value           uniform 0;
    }
    ym
    {
        type            cyclic;
        value           uniform 0;
    }
    xm
    {
        type            cyclic;
        value           uniform 0;
    }
    xp
    {
        type            cyclic;
        value           uniform 0;
    }
    zp
    {
        type            calculated;
        value           uniform 0;
    }
    zm
    {
        type            calculated;
        value           uniform 0;
    }
}


