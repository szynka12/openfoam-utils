/*--------------------------------*- C++ -*----------------------------------*\
| =========                 |                                                 |
| \\      /  F ield         | OpenFOAM: The Open Source CFD Toolbox           |
|  \\    /   O peration     | Version:  v2006                                 |
|   \\  /    A nd           | Website:  www.openfoam.com                      |
|    \\/     M anipulation  |                                                 |
\*---------------------------------------------------------------------------*/
FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs (userFunctionObjects);

dt300     0.005;
dt500     0.003;
dt1000    0.0025;

domain_l    34.5;

fft         #eval{$domain_l};


application     pisoFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         #eval{20*$fft};

deltaT          $dt300;

writeControl    adjustableRunTime;

writeInterval   $fft;

purgeWrite      3;

writeFormat     binary;

writePrecision  16;

writeCompression off;

timeFormat      general;

timePrecision   12;

runTimeModifiable true;

averageControl
{
    timeStart       #eval{5*$fft};
    writeControl    writeTime;
    writeInterval   1;
}

executeControl
{
    executeControl  writeTime;
    executeInterval 1;
}

functions
{
  #include      "post/modelled_fields"
  #includeFunc  "yPlus"
  #include      "post/cd"
  #include      "post/average_Up"

  #include      "post/UPrim"
  
  resolvedK
  {
      type    resolvedKineticEnergy;
      lib     (userFunctionObjects);
      ${/averageControl};
      ${/executeControl};
  }
  
  visRatio
  {
      type    visRatio;
      lib     (userFunctionObjects);
      ${/averageControl};
      ${/executeControl};
  }

  M
  {
      type    M;
      lib     (userFunctionObjects);
      k       turbulenceProperties:k;
      ${/averageControl};
      ${/executeControl};
  }
  
  fieldAverage2
  {
      type            fieldAverage;
      libs            (fieldFunctionObjects);

      fields
      (
          M
          {
              mean        yes;
              prime2Mean  no;
              base        time;
          }
      );
      ${/averageControl}
  }

  #include "post/ax_sample"
  #include "post/probes" 
  #includeFunc "Lambda2"
  #includeFunc "Q"

}


// ************************************************************************* //
