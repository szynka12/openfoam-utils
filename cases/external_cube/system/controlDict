FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     pisoFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         #eval{48*20};

deltaT          0.001;

writeControl    adjustableRunTime;

writeInterval   10;

purgeWrite      3;

writeFormat     binary;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  true;

maxCo           1;

functions
{
  fieldAverage1
  {
      type            fieldAverage;
      libs            (fieldFunctionObjects);

      fields
      (
          U
          {
              mean        yes;
              prime2Mean  yes;
              base        time;
          }
          p
          {
              mean        yes;
              prime2Mean  yes;
              base        time;
         }
      );


      timeStart       #eval{48*5};
      writeControl    writeTime;
      writeInterval   1;
   }
}


// ************************************************************************* //
