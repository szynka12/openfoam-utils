FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    arch        "LSB;label=32;scalar=64";
    location    "30.6";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];


internalField  uniform 0;

boundaryField
{
    sphere
    {
        type            nutUWallFunction;
        value           $internalField;
    }
    "(inlet|outlet)"
    {
        type            calculated;
        value           $internalField;
    }
    "(yp|ym|zp|zm)"
    {
        type            cyclic;
    }
}

