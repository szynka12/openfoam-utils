    cloudSampling 
    {
        type            sets;
        libs            (sampling);
        writeControl    runTime;
        writeInterval   0.3;

        interpolationScheme cell;

        setFormat raw;

        fields
        (
            U p UMean pMean
        );

        sets
        (
            yplus5 
            {
                type        cloud;
                axis        xyz;  
                points 
                (
                    #include yplus_5 
                );
            }
