    wallShearStress1
    {
    	type            wallShearStress;
    	libs            (fieldFunctionObjects);
        ${/functionObjSettings};
    }  


    VelocityPressureAverage
    {
        type            fieldAverage;
        libs            (fieldFunctionObjects);
        
        ${/functionObjSettings};
	      ${/averageSettings};
        fields
        (
            U
            {
                mean        on;
                prime2Mean  off;
                base        time;
            }
            p
            {
                mean        on;
                prime2Mean  off;
                base        time;
            }
	          wallShearStress
	          {
		            mean on;
		            prime2Mean off;
		            base time;
            }
        );
    }
    
