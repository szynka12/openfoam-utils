FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      renumberCoupledPatchesDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

tolerance 1e-3;

patches
(
  {
    master        xp;
    slave         xm;
    slaveToMaster (6.28318530718 0 0);
  }

  {
    master        zp;
    slave         zm;
    slaveToMaster (0 0 3.14159265358979323846264);
  }
);

