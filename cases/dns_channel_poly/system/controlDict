FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

FTT    #eval{ 2*pi()/0.1375 };
FTT2   #eval{ 2*$FTT };
FTT2   #eval{ 2*$FTT };
FTT3   #eval{ 3*$FTT };
FTT4   #eval{ 4*$FTT };
FTT5   #eval{ 5*$FTT };
FTT6   #eval{ 6*$FTT };
FTT10  #eval{ 10*$FTT };
FTT20  #eval{ 20*$FTT };
FTT40  #eval{ 40*$FTT };
FTT60  #eval{ 60*$FTT };
FTT80  #eval{ 80*$FTT };

END #eval{$FTT20};

libs ( userFunctionObjects );

application     pimpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         $END;

deltaT          0.03;

writeControl    adjustableRunTime;

writeInterval   $FTT;

purgeWrite      3;

writeFormat     binary;

writePrecision  12;

writeCompression off;

timeFormat      general;

timePrecision   12;

runTimeModifiable true;

adjustTimeStep  true;

maxCo           0.6;


functionObjSettings
{
    timeStart       $FTT10;
    writeControl    writeTime;
}

averageSettings
{
    //restartOnRestart    false;
}

functions
{
    //#include "runPostDict"  
    #include "avgToTarget"
    
}


